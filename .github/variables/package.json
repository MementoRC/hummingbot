{
  "name": "config",
  "version": "0.1",
  "variables": {
    "MINICONDA_PATH": "/home/hummingbot/miniconda3",
    "RELATIVE_MINICONDA_PATH": "./miniconda3",
    "HB_PYTHON_VERSION": "3.8",
    "HB_ENV_NAME": "hummingbot"
  },
  "architectures": {
    "x86_ubuntu_latest": {
      "name": "x86 Ubuntu Latest",
      "runner": "ubuntu-latest",
      "extra_cmake_args": "-DCMAKE_CXX_FLAGS=-m64 -DCMAKE_C_FLAGS=-m64"
    },
    "x86_ubuntu_2004": {
      "name": "x86 Ubuntu 20.04",
      "runner": "ubuntu-20.04",
      "extra_cmake_args": "-DCMAKE_CXX_FLAGS=-m64 -DCMAKE_C_FLAGS=-m64"
    },
    "x86_macos_latest": {
      "name": "x86 MacOS Latest",
      "runner": "macos-latest",
      "extra_cmake_args": ""
    },
    "x86_windows_latest": {
      "name": "x86 Windows Latest",
      "runner": "windows-latest",
      "extra_cmake_args": ""
    },
    "arm64_ubuntu_latest": {
      "name": "ARM64 Ubuntu Latest",
      "runner": "buildjet-2vcpu-ubuntu_2204-arm",
      "extra_cmake_args": ""
    }
  }
}